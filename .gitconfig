[user]
	name = Will Smith
	email = wsmith85@gmail.com
[core]
	editor = vim
	autocrlf = input
	safecrlf = true
	excludesfile = /Users/wsmith/.gitignore_global
[merge]
	tool = vimdiff
[push]
	default = simple
[alias]
  hist = log --pretty=format:'%h %ad | %s%C(auto)%d [%Cgreen%an%Creset]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
	s = status
	lol = log --oneline --graph --decoreate --all
[color]
	ui = true
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[credential]
	helper = store
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/wsmith/.stCommitMsg
